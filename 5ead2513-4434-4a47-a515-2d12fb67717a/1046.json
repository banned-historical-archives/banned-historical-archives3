34.0196999999997],[230.03999999999996,334.0196999999997]]},{"text":"汉新华农东方红战士","box":[[212.67599999999996,602.8196999999997],[321.6648,602.8196999999997],[321.6648,614.8196999999997],[212.67599999999996,614.8196999999997]]},{"text":"","box":[[321.6648,602.8196999999997],[322.19440000000003,602.8196999999997],[322.19440000000003,602.8196999999997],[321.6648,602.8196999999997]]},{"text":"吴克强《放开我，妈妈！","box":[[328.0199999999999,602.8196999999997],[461.2799999999997,602.8196999999997],[461.2799999999997,614.8196999999997],[328.0199999999999,614.8196999999997]]},{"text":"》","box":[[455.3999999999999,602.8196999999997],[467.3999999999999,602.8196999999997],[467.3999999999999,614.8196999999997],[455.3999999999999,614.8196999999997]]},{"text":"","box":[[467.3999999999999,602.8196999999997],[468.5399999999999,602.8196999999997],[468.5399999999999,602.8196999999997],[467.3999999999999,602.8196999999997]]},{"text":"面对","box":[[481.0799999999999,602.8196999999997],[505.25999999999993,602.8196999999997],[505.25999999999993,614.8196999999997],[481.0799999999999,614.8196999999997]]},{"text":"\"","box":[[130.25999999999993,626.2196999999996],[134.51999999999992,626.2196999999996],[134.51999999999992,638.2196999999996],[130.25999999999993,638.2196999999996]]},{"text":"的大规模屠杀，妈妈怕我到学校去被","box":[[134.51999999999998,626.2197],[326.52,626.2197],[326.52,638.2197],[134.51999999999998,638.2197]]},{"text":"\"","box":[[326.52,626.2197],[330.78,626.2197],[330.78,638.2197],[326.52,638.2197]]},{"text":"百匪","box":[[330.78,626.2197],[354.78,626.2197],[354.78,638.2197],[330.78,638.2197]]},{"text":"\"","box":[[354.78,626.2197],[359.03999999999996,626.2197],[359.03999999999996,638.2197],[354.78,638.2197]]},{"text":"杀害，拉住我。我说：","box":[[359.03999999999996,626.2197],[479.03999999999996,626.2197],[479.03999999999996,638.2197],[359.03999999999996,638.2197]]},{"text":"，","box":[[199.32,696.4196999999999],[211.32,696.4196999999999],[211.32,708.4196999999999],[199.32,708.4196999999999]]},{"text":"得了啥？","box":[[219.852,719.8197],[267.834,719.8197],[267.834,731.8197],[219.852,731.8197]]},{"text":"燕，","box":[[223.332,743.2197],[247.3272,743.2197],[247.3272,755.2197],[223.332,755.2197]]},{"text":"发生打斗，","box":[[90,345.41970000000003],[150,345.41970000000003],[150,357.41970000000003],[90,357.41970000000003]]},{"text":"“","box":[[149.76,345.41970000000003],[153.756,345.41970000000003],[153.756,357.41970000000003],[149.76,357.41970000000003]]},{"text":"百万雄师","box":[[153.78,345.41970000000003],[201.78,345.41970000000003],[201.78,357.41970000000003],[153.78,357.41970000000003]]},{"text":"”","box":[[201.78,345.41970000000003],[205.776,345.41970000000003],[205.776,357.41970000000003],[201.78,357.41970000000003]]},{"text":"不敢用刀对付这些人，另外还有人本身是造反派，又不是","box":[[205.74,345.41970000000003],[505.26,345.41970000000003],[505.26,357.41970000000003],[205.74,357.41970000000003]]},{"text":"头头，是群众，也不许进屋搜，有些屋根本不开门，除五类份子害怕不敢藏学生","box":[[90.00000000000001,368.8197],[505.2407999999999,368.8197],[505.2407999999999,380.8197],[90.00000000000001,380.8197]]},{"text":"外，","box":[[90.00000000000001,392.2197],[114.00000000000001,392.2197],[114.00000000000001,404.2197],[90.00000000000001,404.2197]]},{"text":"“","box":[[113.76000000000002,392.2197],[117.75600000000001,392.2197],[117.75600000000001,404.2197],[113.76000000000002,404.2197]]},{"text":"百万雄师","box":[[117.78000000000002,392.2197],[165.78000000000003,392.2197],[165.78000000000003,404.2197],[117.78000000000002,404.2197]]},{"text":"”","box":[[165.78000000000003,392.2197],[169.77600000000004,392.2197],[169.77600000000004,404.2197],[165.78000000000003,404.2197]]},{"text":"观点的居民尽管自己不藏，但也不敢检举，所以搜查一直到晚上","box":[[169.8,392.2197],[505.32,392.2197],[505.32,404.2197],[169.8,404.2197]]},{"text":"11","box":[[90,415.61969999999997],[102.4296,415.61969999999997],[102.4296,427.61969999999997],[90,427.61969999999997]]},{"text":"","box":[[102.4296,415.61969999999997],[102.72879999999999,415.61969999999997],[102.72879999999999,415.61969999999997],[102.4296,415.61969999999997]]},{"text":"点才结束，搜查出的学生","box":[[106.02,415.61969999999997],[238.07999999999998,415.61969999999997],[238.07999999999998,427.61969999999997],[106.02,427.61969999999997]]},{"text":"","box":[[238.08,415.61969999999997],[238.38000000000002,415.61969999999997],[238.38000000000002,415.61969999999997],[238.08,415.61969999999997]]},{"text":"200","box":[[241.68,415.61969999999997],[261.72720000000004,415.61969999999997],[261.72720000000004,427.61969999999997],[241.68,427.61969999999997]]},{"text":"","box":[[261.72720000000004,415.61969999999997],[262.02660000000003,415.61969999999997],[262.02660000000003,415.61969999999997],[261.72720000000004,415.61969999999997]]},{"text":"余人集体到电车公司，大个子关进大会议室，","box":[[265.32,415.61969999999997],[505.32,415.61969999999997],[505.32,427.61969999999997],[265.32,427.61969999999997]]},{"text":"块头小的关进食堂，先放块头大的边打边审问是否保卫人员","box":[[90,439.01969999999994],[399.65999999999997,439.01969999999994],[399.65999999999997,451.01969999999994],[90,451.01969999999994]]},{"text":"......","box":[[399.65999999999997,439.01969999999994],[423.65999999999997,439.01969999999994],[423.65999999999997,451.01969999999994],[399.65999999999997,451.01969999999994]]},{"text":"此时，区公安局","box":[[423.65999999999997,439.01969999999994],[505.32,439.01969999999994],[505.32,451.01969999999994],[423.65999999999997,451.01969999999994]]},{"text":"出面审讯学生，要他们承认挑起武斗的责任，交待后台。当时，一冶一个叫黄继","box":[[90,462.4196999999999],[505.2407999999998,462.4196999999999],[505.2407999999998,474.4196999999999],[90,474.4196999999999]]},{"text":"元的工人，手制了一把木手枪，平时用来吓唬人，这一次被邴局长抓住，以反革","box":[[90,485.8196999999999],[505.2407999999999,485.8196999999999],[505.2407999999999,497.8196999999999],[90,497.8196999999999]]},{"text":"命持枪杀人犯被捕，","box":[[90,509.2196999999999],[198,509.2196999999999],[198,521.2196999999999],[90,521.2196999999999]]},{"text":"“","box":[[196.62,509.2196999999999],[200.616,509.2196999999999],[200.616,521.2196999999999],[196.62,521.2196999999999]]},{"text":"百万雄师","box":[[200.64000000000001,509.2196999999999],[248.64000000000001,509.2196999999999],[248.64000000000001,521.2196999999999],[200.64000000000001,521.2196999999999]]},{"text":"”","box":[[248.64000000000001,509.2196999999999],[252.63600000000002,509.2196999999999],[252.63600000000002,521.2196999999999],[248.64000000000001,521.2196999999999]]},{"text":"和硚口区公安局连夜审讯并照出的照片，","box":[[252.66000000000003,509.2196999999999],[468.66,509.2196999999999],[468.66,521.2196999999999],[252.66000000000003,521.2196999999999]]},{"text":"7","box":[[467.28000000000003,509.2196999999999],[473.95200000000006,509.2196999999999],[473.95200000000006,521.2196999999999],[467.28000000000003,521.2196999999999]]},{"text":"","box":[[473.95200000000006,509.2196999999999],[474.2010000000001,509.2196999999999],[474.2010000000001,509.2196999999999],[473.95200000000006,509.2196999999999]]},{"text":"月","box":[[476.94000000000005,509.2196999999999],[488.94000000000005,509.2196999999999],[488.94000000000005,521.2196999999999],[476.94000000000005,521.2196999999999]]},{"text":"","box":[[488.94000000000005,509.2196999999999],[489.19500000000005,509.2196999999999],[489.19500000000005,509.2196999999999],[488.94000000000005,509.2196999999999]]},{"text":"17","box":[[492.00000000000006,509.2196999999999],[505.33080000000007,509.2196999999999],[505.33080000000007,521.2196999999999],[492.00000000000006,521.2196999999999]]},{"text":"日在汉口电车公司开展照片展，说明","box":[[90.00000000000006,532.6196999999999],[282.00000000000006,532.6196999999999],[282.00000000000006,544.6196999999999],[90.00000000000006,544.6196999999999]]},{"text":"“7.15”","box":[[282.00000000000006,532.6196999999999],[313.33800000000014,532.6196999999999],[313.33800000000014,544.6196999999999],[282.00000000000006,544.6196999999999]]},{"text":"造反派杀人和破坏电车公司物品，阻","box":[[313.38000000000005,532.6196999999999],[505.3259999999999,532.6196999999999],[505.3259999999999,544.6196999999999],[313.38000000000005,544.6196999999999]]},{"text":"碍公交秩序的事实。","box":[[90.00000000000006,556.0196999999998],[198.00000000000006,556.0196999999998],[198.00000000000006,568.0196999999998],[90.00000000000006,568.0196999999998]]},{"text":"一九六七年七月，武","box":[[103.56000000000006,602.8196999999998],[212.5488000000001,602.8196999999998],[212.5488000000001,614.8196999999998],[103.56000000000006,614.8196999999998]]},{"text":"着","box":[[90.00000000000006,626.2196999999999],[102.00000000000006,626.2196999999999],[102.00000000000006,638.2196999999999],[90.00000000000006,638.2196999999999]]},{"text":"\"","box":[[102.00000000000006,626.2196999999999],[106.26000000000006,626.2196999999999],[106.26000000000006,638.2196999999999],[102.00000000000006,638.2196999999999]]},{"text":"百匪","box":[[106.26000000000006,626.2196999999999],[130.26000000000005,626.2196999999999],[130.26000000000005,638.2196999999999],[106.26000000000006,638.2196999999999]]},{"text":"放开我，妈妈！","box":[[103.38000000000005,649.6196999999999],[187.32960000000008,649.6196999999999],[187.32960000000008,661.6196999999999],[103.38000000000005,661.6196999999999]]},{"text":"别为孩儿担惊受怕。","box":[[103.38000000000005,673.0196999999998],[211.32239999999996,673.0196999999998],[211.32239999999996,685.0196999999998],[103.38000000000005,685.0196999999998]]},{"text":"我们的战友遍天下","box":[[103.38000000000005,696.4196999999998],[199.32959999999997,696.4196999999998],[199.32959999999997,708.4196999999998],[103.38000000000005,708.4196999999998]]},{"text":"\"","box":[[103.38720000000006,719.8196999999998],[107.64720000000007,719.8196999999998],[107.64720000000007,731.8196999999998],[103.38720000000006,731.8196999999998]]},{"text":"百匪","box":[[107.64000000000006,719.8196999999998],[131.64000000000004,719.8196999999998],[131.64000000000004,731.8196999999998],[107.64000000000006,731.8196999999998]]},{"text":"\"","box":[[131.64000000000004,719.8196999999998],[135.90000000000003,719.8196999999998],[135.90000000000003,731.8196999999998],[131.64000000000004,731.8196999999998]]},{"text":"的长矛、匕首算","box":[[135.90000000000003,719.8196999999998],[219.86400000000003,719.819