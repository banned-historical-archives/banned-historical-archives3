1.61969999999985]]},{"text":"毛","box":[[175.32,181.61969999999985],[187.32,181.61969999999985],[187.32,193.61969999999985],[175.32,193.61969999999985]]},{"text":"","box":[[187.32,181.61969999999985],[188.32,181.61969999999985],[188.32,181.61969999999985],[187.32,181.61969999999985]]},{"text":"泽","box":[[199.32,181.61969999999985],[211.32,181.61969999999985],[211.32,193.61969999999985],[199.32,193.61969999999985]]},{"text":"","box":[[211.32,181.61969999999985],[212.32,181.61969999999985],[212.32,181.61969999999985],[211.32,181.61969999999985]]},{"text":"东","box":[[223.32,181.61969999999985],[235.32,181.61969999999985],[235.32,193.61969999999985],[223.32,193.61969999999985]]},{"text":"","box":[[235.32,181.61969999999985],[237.32,181.61969999999985],[237.32,181.61969999999985],[235.32,181.61969999999985]]},{"text":"八月七日下午六时","box":[[259.32,181.61969999999985],[355.32,181.61969999999985],[355.32,193.61969999999985],[259.32,193.61969999999985]]},{"text":"","box":[[355.32,181.61969999999985],[357.32,181.61969999999985],[357.32,181.61969999999985],[355.32,181.61969999999985]]},{"text":"根据中共中央文件刊印。","box":[[379.32,181.61969999999985],[511.32,181.61969999999985],[511.32,193.61969999999985],[379.32,193.61969999999985]]},{"text":"三","box":[[90,205.01969999999983],[102,205.01969999999983],[102,217.01969999999983],[90,217.01969999999983]]},{"text":"","box":[[102,205.01969999999983],[104,205.01969999999983],[104,205.01969999999983],[102,205.01969999999983]]},{"text":"除了军区负责之外，中央文革小组对湖南省军区二月三日关于","box":[[126,205.01969999999983],[449.28,205.01969999999983],[449.28,217.01969999999983],[126,217.01969999999983]]},{"text":"“","box":[[449.28,205.01969999999983],[453.27599999999995,205.01969999999983],[453.27599999999995,217.01969999999983],[449.28,217.01969999999983]]},{"text":"湘江风雷","box":[[453.29999999999995,205.01969999999983],[501.29999999999995,205.01969999999983],[501.29999999999995,217.01969999999983],[453.29999999999995,217.01969999999983]]},{"text":"”","box":[[501.29999999999995,205.01969999999983],[505.29599999999994,205.01969999999983],[505.29599999999994,217.01969999999983],[501.29999999999995,217.01969999999983]]},{"text":"报告所发的","box":[[90,228.4196999999998],[150,228.4196999999998],[150,240.4196999999998],[90,240.4196999999998]]},{"text":"“","box":[[150,228.4196999999998],[153.996,228.4196999999998],[153.996,240.4196999999998],[150,240.4196999999998]]},{"text":"二","box":[[154.01999999999998,228.4196999999998],[166.01999999999998,228.4196999999998],[166.01999999999998,240.4196999999998],[154.01999999999998,240.4196999999998]]},{"text":"·","box":[[166.01999999999998,228.4196999999998],[169.356,228.4196999999998],[169.356,240.4196999999998],[166.01999999999998,240.4196999999998]]},{"text":"四批示","box":[[169.38,228.4196999999998],[205.38,228.4196999999998],[205.38,240.4196999999998],[169.38,240.4196999999998]]},{"text":"”","box":[[205.38,228.4196999999998],[209.376,228.4196999999998],[209.376,240.4196999999998],[205.38,240.4196999999998]]},{"text":"是错误的，","box":[[209.34,228.4196999999998],[269.34000000000003,228.4196999999998],[269.34000000000003,240.4196999999998],[209.34,240.4196999999998]]},{"text":"“","box":[[269.34000000000003,228.4196999999998],[273.336,228.4196999999998],[273.336,240.4196999999998],[269.34000000000003,240.4196999999998]]},{"text":"湘江风雷","box":[[273.36,228.4196999999998],[321.36,228.4196999999998],[321.36,240.4196999999998],[273.36,240.4196999999998]]},{"text":"”","box":[[321.36,228.4196999999998],[325.356,228.4196999999998],[325.356,240.4196999999998],[321.36,240.4196999999998]]},{"text":"是一个群众人数较多的革命组织，","box":[[325.32,228.4196999999998],[505.32,228.4196999999998],[505.32,240.4196999999998],[325.32,240.4196999999998]]},{"text":"在大发展中混入了极少数的坏人，造成内部成分不纯，出现了一些缺点错误，但","box":[[90,251.81969999999978],[505.31999999999994,251.81969999999978],[505.31999999999994,263.8196999999998],[90,263.8196999999998]]},{"text":"是不应该因此否定这个组织。","box":[[90,275.21969999999976],[246,275.21969999999976],[246,287.21969999999976],[90,287.21969999999976]]},{"text":"在这个问题上，","box":[[242.46,275.21969999999976],[326.46000000000004,275.21969999999976],[326.46000000000004,287.21969999999976],[242.46,287.21969999999976]]},{"text":"中央是有责任的。","box":[[322.92,275.21969999999976],[418.92,275.21969999999976],[418.92,287.21969999999976],[322.92,287.21969999999976]]},{"text":"从这里得到教训，","box":[[415.38,275.21969999999976],[511.38,275.21969999999976],[511.38,287.21969999999976],[415.38,287.21969999999976]]},{"text":"未经调查清楚，何必那样匆匆忙忙地根据一面之词发出","box":[[90,298.61969999999974],[375.96000000000004,298.61969999999974],[375.96000000000004,310.61969999999974],[90,310.61969999999974]]},{"text":"“","box":[[375.96000000000004,298.61969999999974],[379.956,298.61969999999974],[379.956,310.61969999999974],[375.96000000000004,310.61969999999974]]},{"text":"二","box":[[379.98,298.61969999999974],[391.98,298.61969999999974],[391.98,310.61969999999974],[379.98,310.61969999999974]]},{"text":"·","box":[[391.98,298.61969999999974],[395.31600000000003,298.61969999999974],[395.31600000000003,310.61969999999974],[391.98,310.61969999999974]]},{"text":"四批示","box":[[395.34000000000003,298.61969999999974],[431.34000000000003,298.61969999999974],[431.34000000000003,310.61969999999974],[395.34000000000003,310.61969999999974]]},{"text":"”","box":[[431.34000000000003,298.61969999999974],[435.336,298.61969999999974],[435.336,310.61969999999974],[431.34000000000003,310.61969999999974]]},{"text":"呢？因此，中","box":[[435.3,298.61969999999974],[505.26,298.61969999999974],[505.26,310.61969999999974],[435.3,310.61969999999974]]},{"text":"央要负主要的责任。","box":[[90.00000000000006,322.0196999999997],[198.00000000000006,322.0196999999997],[198.00000000000006,334.0196999999997],[90.00000000000006,334.0196999999997]]},{"text":"","box":[[198.00000000000006,322.0196999999997],[199.70500000000007,322.0196999999997],[199.70500000000007,322.0196999999997],[198.00000000000006,322.0196999999997]]},{"text":"根据中共中央文件刊印。","box":[[218.46000000000006,322.0196999999997],[350.46000000000004,322.0196999999997],[350.46000000000004,334.0196999999997],[218.46000000000006,334.0196999999997]]},{"text":"（有毛泽东修改件）","box":[[340.92,322.0196999999997],[448.92,322.0196999999997],[448.92,334.0196999999997],[340.92,334.0196999999997]]},{"text":"","box":[[448.92,322.0196999999997],[450.625,322.0196999999997],[450.625,322.0196999999997],[448.92,322.0196999999997]]},{"text":"注","box":[[469.38000000000005,322.0196999999997],[481.38000000000005,322.0196999999997],[481.38000000000005,334.0196999999997],[469.38000000000005,334.0196999999997]]},{"text":"","box":[[481.38000000000005,322.0196999999997],[482.38000000000005,322.0196999999997],[482.38000000000005,322.0196999999997],[481.38000000000005,322.0196999999997]]},{"text":"释","box":[[493.38000000000005,322.0196999999997],[505.38000000000005,322.0196999999997],[505.38000000000005,334.0196999999997],[493.38000000000005,334.0196999999997]]},{"text":"〔１〕","box":[[90.00000000000006,345.4196999999997],[126.36000000000007,345.4196999999997],[126.36000000000007,357.4196999999997],[90.00000000000006,357.4196999999997]]},{"text":"","box":[[126.36000000000007,345.4196999999997],[127.39000000000007,345.4196999999997],[127.39000000000007,345.4196999999997],[126.36000000000007,345.4196999999997]]},{"text":"中共中央关于湖南问题的若干决定稿共四条，主要是：","box":[[138.72000000000008,345.4196999999997],[430.86000000000024,345.4196999999997],[430.86000000000024,357.4196999999997],[138.72000000000008,357.4196999999997]]},{"text":"","box":[[430.86000000000024,345.4196999999997],[432.0000000000002,345.4196999999997],[432.0000000000002,345.4196999999997],[430.86000000000024,345.4196999999997]]},{"text":"一、湖南省","box":[[444.5400000000001,345.4196999999997],[505.2600000000001,345.4196999999997],[505.2600000000001,357.4196999999997],[444.5400000000001,357.4196999999997]]},{"text":"军区党委常委《关于支左工作中所犯错误的检查报告》对前段支左工作进行了认","box":[[90.00000000000006,368.81969999999967],[505.32000000000005,368.81969999999967],[505.32000000000005,380.81969999999967],[90.00000000000006,380.81969999999967]]},{"text":"真的自我批评，态度是好的，认识是比较深刻的。","box":[[90.00000000000006,392.21969999999965],[351.6504000000002,392.21969999999965],[351.6504000000002,404.21969999999965],[90.00000000000006,404.21969999999965]]},{"text":"","box":[[351.65040000000016,392.21969999999965],[353.55100000000016,392.21969999999965],[353.55100000000016,392.21969999999965],[351.65040000000016,392.21969999999965]]},{"text":"二、中央已经决定改组省","box":[[374.4576000000002,392.21969999999965],[505.28160000000037,392.21969999999965],[505.28160000000037,404.21969999999965],[374.4576000000002,404.21969999999965]]},{"text":"军区，并着手成立湖南省革命委员会筹备小组，领导全省的文化大革命和工农业","box":[[90.00000000000006,415.6196999999996],[505.32000000000005,415.6196999999996],[505.32000000000005,427.6196999999996],[90.00000000000006,427.6196999999996]]},{"text":"生产。","box":[[90.00000000000006,439.0196999999996],[126.00000000000006,439.0196999999996],[126.00000000000006,451.0196999999996],[90.00000000000006,451.0196999999996]]},{"text":"三、各革命群众组织应在省革委筹备小组的领导下，在毛主席无产阶级革命","box":[[103.56000000000006,462.4196999999996],[505.24319999999966,462.4196999999996],[505.24319999999966,474.4196999999996],[103.56000000000006,474.4196999999996]]},{"text":"路线基础上加强团结，开展革命的大批判，狠抓革命，猛促生产，保证完成和超","box":[[90.00000000000006,485.81969999999956],[505.24079999999987,485.81969999999956],[505.24079999999987,497.81969999999956],[90.00000000000006,497.81969999999956]]},{"text":"额完成今年的工农业生产任务。","box":[[90.00000000000006,509.21969999999953],[258.00000000000006,509.21969999999953],[258.00000000000006,521.2196999999995],[90.00000000000006,521.2196999999995]]},{"text":"四、湖南省赴京汇报代表团双方签订的《关于立即坚决制止武斗协议》很好，","box":[[103.38000000000005,532.6196999999995],[511.3404000000001,532.6196999999995],[511.3404000000001,544.6196999999995],[103.38000000000005,544.6196999999995]]},{"text":"各群众组织必须认真执行。","box":[[90.00000000000006,556.0196999999995],[234.00000000000006,556.0196999999995],[234.00000000000006,568.0196999999995],[90.00000000000006,568.0196999999995]]},{"text":"中央决定稿曾两次送毛泽东审阅，本篇一、二是分别对这两次送审稿的批语；","box":[[103.38000000000005,579.4196999999995],[511.26120000000003,579.4196999999995],[511.26120000000003,591.4196999999995],[103.38000000000005,591.4196999999995]]},{"text":"本篇三是对这个决定稿的修改，其中用宋体字排印的是毛泽东加写和改写的文","box":[[90.00000000000006,602.8196999999994],[505.4052,602.8196999999994],[505.4052,614.8196999999994],[90.00000000000006,614.8196999999994]]},{"text":"字。一九六七年八月十日，中共中央下发了这个决定。","box":[[90.00000000000006,626.2196999999994],[378.00000000000006,626.2196999999994],[378.00000000000006,638.2196999999994],[90.00000000000006,638.2196999999994]]},{"text":"一九六七年八月六日，戚本禹接见湖南","box":[[103.38000000000005,649.6196999999995],[307.3223999999999,649.6196999999995],[307.3223999999999,661.6196999999995],[103.38000000000005,661.6196999999995]]},{"text":"“","box":[[307.31879999999995,649.6196999999995],[311.31479999999993,649.6196999999995],[311.31479999999993,661.6196999999995],[307.31879999999995,661.6196999999995]]},{"text":"红联","box":[[311.34000000000003,649.6196999999995],[335.34000000000003,649.6196999999995],[335.34000000000003,661.6196999999995],[311.34000000000003,661.6196999999995]]},{"text":"”","box":[[335.34000000000003,649.6196999999995],[339.336,649.6196999999995],[339.336,661.6196999999995],[335.34000000000003,661.6196999999995]]},{"text":"代表谈话纪要","box":[[339.36,649.6196999999995],[411.36,649.6196999999995],[411.36,661.6196999999995],[339.36,661.6196999999995]]},{"text":"零点四十六分至三点半，戚本禹同志接见","box":[[103.38,673.0196999999995],[319.3187999999999,673.0196999999995],[319.3187999999999,685.0196999999995],[103.38,685.0196999999995]]},{"text":"“","box":[[319.3151999999999,673.0196999999995],[323.3111999999999,673.0196999999995],[323.3111999999999,685.0196999999995],[319.3151999999999,685.0196999999995]]},{"text":"红联","box":[[323.34,673.0196999999995],[347.34,673.0196999999995],[347.34,685.0196999999995],[323.34,685.0196999999995]]},{"text":"”","box":[[347.34,673.0196999999995],[351.33599999999996,673.0196999999995],[351.33599999999996,685.0196999999995],[347.34,685.0196999999995]]},{"text":"代表","box":[[351.35999999999996,673.0196999999995],[375.35999999999996,673.0196999999995],[375.35999999999996,685.0196999999995],[351.35999999999996,685.0196999999995]]},{"text":"(","box":[[375.35999999999996,673.0196999999995],[379.35599999999994,673.0196999999995],[379.35599999999994,685.0196999999995],[375.35999999999996,685.0196999999995]]},{"text":"包括专、市","box":[[379.31999999999994,673.0196999999995],[439.31999999999994,673.0196999999995],[439.31999999999994,685.0196999999995],[379.31999999999994,685.0196999999995]]},{"text":")","box":[[439.31999999999994,673.0196999999995],[443.3159999999999,673.0196999999995],[443.3159999999999,685.0196999999995],[439.31999999999994,685.0196999999995]]},{"text":"全体成员。","box":[[443.3399999999999,673.0196999999995],[503.3399999999999,673.0196999999995],[503.3399999999999,685.0196999999995],[443.3399999999999,685.0196999999995]]},{"text":"戚本禹同志拿着名册一一点名，当点到胡少奇时，戚本禹同志说：你叫胡少","box":[[103.55999999999989,696.4196999999995],[505.2431999999995,696.4196999999995],[505.2431999999995,708.4196999999995],[103.55999999999989,708.4196999999995]]},{"text":"奇，这次你回去干了一件坏事。","box":[[89.99999999999989,719.8196999999996],[257.9999999999999,719.8196999999996],[257.9999999999999,731.8196999999996],[89.99999999999989,731.8196999999996]]},{"text":"胡少奇：报告戚本禹同志，我正要同你汇报。","box":[[103.37999999999988,743.2196999999995],[343.31159999999977,743.2196999999995],[343.31159999999977,755.2196999999995],[103.37999999999988,755.2196999999995]]},{"text":"第660页，共4822页","box":[[397.2117615,34.45633350000003],[523.2117615,34.45633350000003],[523.2117615,46.45633350000003],[397.2117615,46.45633350000003]]}]}