340000000001,217.01969999999983]]},{"text":"","box":[[276.45599999999996,205.01969999999983],[278.46739999999994,205.01969999999983],[278.46739999999994,205.01969999999983],[276.45599999999996,205.01969999999983]]},{"text":"呈报：中共四川省自贡市委王海林书记","box":[[300.5927999999999,205.01969999999983],[505.23479999999995,205.01969999999983],[505.23479999999995,217.01969999999983],[300.5927999999999,217.01969999999983]]},{"text":"市政府雷洪金市长","box":[[90.00000000000006,228.4196999999998],[187.40280000000007,228.4196999999998],[187.40280000000007,240.4196999999998],[90.00000000000006,240.4196999999998]]},{"text":"","box":[[187.40280000000007,228.4196999999998],[190.48730000000006,228.4196999999998],[190.48730000000006,228.4196999999998],[187.40280000000007,228.4196999999998]]},{"text":"市人大王海林主任","box":[[224.41680000000008,228.4196999999998],[321.83040000000005,228.4196999999998],[321.83040000000005,240.4196999999998],[224.41680000000008,240.4196999999998]]},{"text":"","box":[[321.83040000000005,228.4196999999998],[322.87960000000004,228.4196999999998],[322.87960000000004,228.4196999999998],[321.83040000000005,228.4196999999998]]},{"text":"市人大党组书记高先敏","box":[[334.42080000000004,228.4196999999998],[456.192,228.4196999999998],[456.192,240.4196999999998],[334.42080000000004,240.4196999999998]]},{"text":"","box":[[456.192,228.4196999999998],[457.2412,228.4196999999998],[457.2412,228.4196999999998],[456.192,228.4196999999998]]},{"text":"市政协","box":[[468.7824,228.4196999999998],[505.176,228.4196999999998],[505.176,240.4196999999998],[468.7824,240.4196999999998]]},{"text":"主席梁国定","box":[[90.00000000000006,251.81969999999978],[150.00000000000006,251.81969999999978],[150.00000000000006,263.8196999999998],[90.00000000000006,263.8196999999998]]},{"text":"","box":[[150.00000000000006,251.81969999999978],[152.00000000000006,251.81969999999978],[152.00000000000006,251.81969999999978],[150.00000000000006,251.81969999999978]]},{"text":"抄报：","box":[[174.00000000000006,251.81969999999978],[210.00000000000006,251.81969999999978],[210.00000000000006,263.8196999999998],[174.00000000000006,263.8196999999998]]},{"text":"中共自贡市委办公厅秘书长","box":[[205.32000000000005,251.81969999999978],[349.32000000000005,251.81969999999978],[349.32000000000005,263.8196999999998],[205.32000000000005,263.8196999999998]]},{"text":"","box":[[349.32000000000005,251.81969999999978],[350.32000000000005,251.81969999999978],[350.32000000000005,251.81969999999978],[349.32000000000005,251.81969999999978]]},{"text":"中共自贡市政法委书记王建","box":[[361.32000000000005,251.81969999999978],[505.32000000000005,251.81969999999978],[505.32000000000005,263.8196999999998],[361.32000000000005,263.8196999999998]]},{"text":"威","box":[[90.00000000000006,275.21969999999976],[102.00000000000006,275.21969999999976],[102.00000000000006,287.21969999999976],[90.00000000000006,287.21969999999976]]},{"text":"","box":[[102.00000000000006,275.21969999999976],[107.14020000000006,275.21969999999976],[107.14020000000006,275.21969999999976],[102.00000000000006,275.21969999999976]]},{"text":"自贡市政府办公厅秘书长","box":[[163.68240000000011,275.21969999999976],[297.5784000000002,275.21969999999976],[297.5784000000002,287.21969999999976],[163.68240000000011,287.21969999999976]]},{"text":"","box":[[297.57840000000016,275.21969999999976],[302.7194000000002,275.21969999999976],[302.7194000000002,275.21969999999976],[297.57840000000016,275.21969999999976]]},{"text":"四川省委、省府信访办领导","box":[[359.2704000000003,275.21969999999976],[505.36920000000055,275.21969999999976],[505.36920000000055,287.21969999999976],[359.2704000000003,287.21969999999976]]},{"text":"自贡市人民政府信访局李剑副秘书长兼局长、阮恒远副局长","box":[[90.00000000000017,298.61969999999974],[402.00000000000017,298.61969999999974],[402.00000000000017,310.61969999999974],[90.00000000000017,310.61969999999974]]},{"text":"二00九年五月三十日，张闯“反革命案”申诉状","box":[[114.00000000000017,345.4196999999997],[372.00000000000017,345.4196999999997],[372.00000000000017,357.4196999999997],[114.00000000000017,357.4196999999997]]},{"text":"1975，9.11我就被省公安厅特捕了。罪名也与时俱进，一会是邓的社会","box":[[126.16200000000018,368.81969999999967],[505.2899999999996,368.81969999999967],[505.2899999999996,380.81969999999967],[126.16200000000018,380.81969999999967]]},{"text":"基础，一时又是四人帮的爪牙；一时是组织《马列主义研究会》的反革命首犯，","box":[[90.00000000000017,392.21969999999965],[510.00000000000017,392.21969999999965],[510.00000000000017,404.21969999999965],[90.00000000000017,404.21969999999965]]},{"text":"受四川日报大批判。","box":[[90.00000000000017,415.6196999999996],[198.00000000000017,415.6196999999996],[198.00000000000017,427.6196999999996],[90.00000000000017,427.6196999999996]]},{"text":"一会儿又是释放在押犯的现形反革命分子。","box":[[192.66000000000017,415.6196999999996],[420.6600000000002,415.6196999999996],[420.6600000000002,427.6196999999996],[192.66000000000017,427.6196999999996]]},{"text":"真是嘴有两块皮，","box":[[415.3200000000002,415.6196999999996],[511.3200000000002,415.6196999999996],[511.3200000000002,427.6196999999996],[415.3200000000002,427.6196999999996]]},{"text":"欲加之罪，何患无词！申诉了30多年，去年才有两个相互推诿的川渝高院的回","box":[[90.00000000000017,439.0196999999996],[505.32000000000016,439.0196999999996],[505.32000000000016,451.0196999999996],[90.00000000000017,451.0196999999996]]},{"text":"答。由此可见，清理反革命帮派体系的实质，不过是走资派的反攻倒算。我被定","box":[[90.00000000000017,462.4196999999996],[505.2408000000001,462.4196999999996],[505.2408000000001,474.4196999999996],[90.00000000000017,474.4196999999996]]},{"text":"为反革命帮派，并不意外。","box":[[90.00000000000017,485.81969999999956],[234.00000000000017,485.81969999999956],[234.00000000000017,497.81969999999956],[90.00000000000017,497.81969999999956]]},{"text":"（张闯）","box":[[228.00000000000017,485.81969999999956],[276.00000000000017,485.81969999999956],[276.00000000000017,497.81969999999956],[228.00000000000017,497.81969999999956]]},{"text":"","box":[[276.00000000000017,485.81969999999956],[278.00000000000017,485.81969999999956],[278.00000000000017,485.81969999999956],[276.00000000000017,485.81969999999956]]},{"text":"张闯“反革命案”申诉状","box":[[300.00000000000017,485.81969999999956],[432.00000000000017,485.81969999999956],[432.00000000000017,497.81969999999956],[300.00000000000017,497.81969999999956]]},{"text":"刑事申诉状","box":[[90.00000000000017,509.21969999999953],[150.00960000000015,509.21969999999953],[150.00960000000015,521.2196999999995],[90.00000000000017,521.2196999999995]]},{"text":"","box":[[150.00960000000015,509.21969999999953],[152.01060000000015,509.21969999999953],[152.01060000000015,509.21969999999953],[150.00960000000015,509.21969999999953]]},{"text":"申诉人：张闯，又名张正富，男，1944年7月29日出生，汉族，","box":[[174.02160000000012,509.21969999999953],[511.26480000000026,509.21969999999953],[511.26480000000026,521.2196999999995],[174.02160000000012,521.2196999999995]]},{"text":"大学文化，","box":[[90.00000000000017,532.6196999999995],[150.00000000000017,532.6196999999995],[150.00000000000017,544.6196999999995],[90.00000000000017,544.6196999999995]]},{"text":"原为四川省革委会委员，","box":[[146.46000000000018,532.6196999999995],[278.46000000000015,532.6196999999995],[278.46000000000015,544.6196999999995],[146.46000000000018,544.6196999999995]]},{"text":"后在万县地区科委工作，","box":[[274.92000000000013,532.6196999999995],[406.92000000000013,532.6196999999995],[406.92000000000013,544.6196999999995],[274.92000000000013,544.6196999999995]]},{"text":"被判刑释放后无业，","box":[[403.3800000000001,532.6196999999995],[511.3800000000001,532.6196999999995],[511.3800000000001,544.6196999999995],[403.3800000000001,544.6196999999995]]},{"text":"住四川省南充市金鱼岭正街216号附3号。","box":[[90.00000000000017,556.0196999999995],[318.00000000000017,556.0196999999995],[318.00000000000017,568.0196999999995],[90.00000000000017,568.0196999999995]]},{"text":"","box":[[318.00000000000017,556.0196999999995],[319.8700000000002,556.0196999999995],[319.8700000000002,556.0196999999995],[318.00000000000017,556.0196999999995]]},{"text":"委托代理人：陈继才，重庆渝万","box":[[340.44000000000017,556.0196999999995],[505.32000000000016,556.0196999999995],[505.32000000000016,568.0196999999995],[340.44000000000017,568.0196999999995]]},{"text":"律师事务所律师。","box":[[90.00000000000017,579.4196999999995],[186.00000000000017,579.4196999999995],[186.00000000000017,591.4196999999995],[90.00000000000017,591.4196999999995]]},{"text":"","box":[[186.00000000000017,579.4196999999995],[187.90000000000018,579.4196999999995],[187.90000000000018,579.4196999999995],[186.00000000000017,579.4196999999995]]},{"text":"申诉人张正富因在“文革”后期被控以反革命罪一案，对","box":[[208.80000000000018,579.4196999999995],[505.2600000000002,579.4196999999995],[505.2600000000002,591.4196999999995],[208.80000000000018,591.4196999999995]]},{"text":"四川省高级人民法院1980年2月27日作出的（80）川法刑一字第28号刑事裁","box":[[90.00000000000017,602.8196999999994],[505.3116000000004,602.8196999999994],[505.3116000000004,614.8196999999994],[90.00000000000017,614.8196999999994]]},{"text":"定书不服，现提出申诉。","box":[[90.0000000000002,626.2196999999994],[222.39600000000024,626.2196999999994],[222.39600000000024,638.2196999999994],[90.0000000000002,638.2196999999994]]},{"text":"","box":[[222.39600000000024,626.2196999999994],[224.40850000000023,626.2196999999994],[224.40850000000023,626.2196999999994],[222.39600000000024,626.2196999999994]]},{"text":"请求事项：","box":[[246.54600000000028,626.2196999999994],[306.7044000000003,626.2196999999994],[306.7044000000003,638.2196999999994],[246.54600000000028,638.2196999999994]]},{"text":"","box":[[306.7044000000003,626.2196999999994],[308.7209000000003,626.2196999999994],[308.7209000000003,626.2196999999994],[306.7044000000003,626.2196999999994]]},{"text":"1、请求撤销四川省高级人民法院","box":[[330.90240000000034,626.2196999999994],[505.39680000000044,626.2196999999994],[505.39680000000044,638.2196999999994],[330.90240000000034,638.2196999999994]]},{"text":"（80）川法刑一字第28号刑事裁定书，为申诉人恢复名誉。","box":[[90.0000000000002,649.6196999999995],[405.6324000000003,649.6196999999995],[405.6324000000003,661.6196999999995],[90.0000000000002,661.6196999999995]]},{"text":"","box":[[405.63240000000013,649.6196999999995],[407.53290000000015,649.6196999999995],[407.53290000000015,649.6196999999995],[405.63240000000013,649.6196999999995]]},{"text":"2、对申诉人在","box":[[428.43840000000006,649.6196999999995],[505.24559999999997,649.6196999999995],[505.24559999999997,661.6196999999995],[428.43840000000006,661.6196999999995]]},{"text":"服刑期间所造成的物质损失进行赔偿。","box":[[90.0000000000002,673.0196999999995],[294.0000000000002,673.0196999999995],[294.0000000000002,685.0196999999995],[90.0000000000002,685.0196999999995]]},{"text":"","box":[[294.0000000000002,673.0196999999995],[295.87000000000023,673.0196999999995],[295.87000000000023,673.0196999999995],[294.0000000000002,673.0196999999995]]},{"text":"事实及理由：","box":[[316.4400000000002,673.0196999999995],[388.4400000000002,673.0196999999995],[388.4400000000002,685.0196999999995],[316.4400000000002,685.0196999999995]]},{"text":"","box":[[388.4400000000002,673.0196999999995],[390.31000000000023,673.0196999999995],[390.31000000000023,673.0196999999995],[388.4400000000002,673.0196999999995]]},{"text":"申诉人张闯在“文","box":[[410.8800000000002,673.0196999999995],[505.3200000000002,673.0196999999995],[505.3200000000002,685.0196999999995],[410.8800000000002,685.0196999999995]]},{"text":"革”后期，被控以思想反动，先后于1970年和1975年两次被捕，后一次一直关","box":[[90.0000000000002,696.4196999999995],[505.2600000000002,696.4196999999995],[505.2600000000002,708.4196999999995],[90.0000000000002,708.4196999999995]]},{"text":"押到1979年底，经由四川省人民检察院万县分院于1979年12月30日出具起","box":[[90.0000000000002,719.8196999999996],[505.2600000000002,719.8196999999996],[505.2600000000002,731.8196999999996],[90.0000000000002,731.8196999999996]]},{"text":"诉书，四川省万县地区中级人民法院未将张闯交付法庭审判，便于次日（1979","box":[[90.0000000000002,743.2196999999995],[505.32000000000016,743.2196999999995],[505.32000000000016,755.2196999999995],[90.0000000000002,755.2196999999995]]},{"text":"第1696页，共1862页","box":[[391.2117615,34.45633350000003],[523.2117615,34.45633350000003],[523.2117615,46.45633350000003],[391.2117615,46.45633350000003]]}]}