9995],[147.84000000000017,532.6196999999995],[147.84000000000017,544.6196999999995],[111.84000000000017,544.6196999999995]]},{"text":"\"","box":[[147.84000000000017,532.6196999999995],[152.73600000000016,532.6196999999995],[152.73600000000016,544.6196999999995],[147.84000000000017,544.6196999999995]]},{"text":"交","box":[[152.76000000000016,532.6196999999995],[164.76000000000016,532.6196999999995],[164.76000000000016,544.6196999999995],[152.76000000000016,544.6196999999995]]},{"text":"\"","box":[[164.76000000000016,532.6196999999995],[169.65600000000018,532.6196999999995],[169.65600000000018,544.6196999999995],[164.76000000000016,544.6196999999995]]},{"text":"，两个","box":[[169.68000000000015,532.6196999999995],[205.68000000000015,532.6196999999995],[205.68000000000015,544.6196999999995],[169.68000000000015,544.6196999999995]]},{"text":"\"","box":[[205.68000000000015,532.6196999999995],[210.57600000000014,532.6196999999995],[210.57600000000014,544.6196999999995],[205.68000000000015,544.6196999999995]]},{"text":"莫高","box":[[210.60000000000014,532.6196999999995],[234.60000000000014,532.6196999999995],[234.60000000000014,544.6196999999995],[210.60000000000014,544.6196999999995]]},{"text":"\"","box":[[234.60000000000014,532.6196999999995],[239.49600000000015,532.6196999999995],[239.49600000000015,544.6196999999995],[234.60000000000014,544.6196999999995]]},{"text":"后，原文都有","box":[[239.52000000000015,532.6196999999995],[311.52000000000015,532.6196999999995],[311.52000000000015,544.6196999999995],[239.52000000000015,544.6196999999995]]},{"text":"\"","box":[[311.52000000000015,532.6196999999995],[316.41600000000017,532.6196999999995],[316.41600000000017,544.6196999999995],[311.52000000000015,544.6196999999995]]},{"text":"于","box":[[316.44000000000017,532.6196999999995],[328.44000000000017,532.6196999999995],[328.44000000000017,544.6196999999995],[316.44000000000017,544.6196999999995]]},{"text":"\"","box":[[328.44000000000017,532.6196999999995],[333.3360000000002,532.6196999999